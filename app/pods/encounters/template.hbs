{{#ember-wormhole to='sidebar'}}
  <h2>
    Encounters
    {{#link-to 'encounters.details' 'new' classNames='icon'}}{{icon 'add'}}{{/link-to}}
  </h2>
  <ul>
    {{#each (sort-by 'name' (reject-by 'isNew' true model.encounters)) as |encounter|}}
      <li>
        {{#link-to 'encounters.details' encounter.id}}{{encounter.name}}{{/link-to}}
      </li>
    {{/each}}
  </ul>
{{/ember-wormhole}}
{{liquid-outlet enableGrowth=false}}
